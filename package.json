{
  "name": "kaizer",
  "displayName": "kaizer",
  "license": "MIT",
  "repository": "https://github.com/polarvoid/kaizer-pets",
  "contributors": ["Elisha Estibeiro", "Alvin Philips"],
  "author": {"name": "Alvin Philips", "email": "alvinphilips257@gmail.com"},
  "description": "A virtual pet to keep you company while coding :)",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:kaizer.initializePet"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kaizer.initializePet",
        "category": "Kaizer",
        "title": "Initialize"
      },
      {
        "command": "kaizer.feedPet",
        "category": "Kaizer",
        "title": "Feed Pet"
      },
      {
        "command": "kaizer.specialMove",
        "category": "Kaizer",
        "title": "Perform Special Move"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
